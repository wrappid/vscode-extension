{
  "name": "wrppidextension",
  "displayName": "WrppidExtension",
  "publisher": "wrappid",
  "description": "This is the README for your extension \"wrppidextension\". After writing up a brief description, we recommend including the following sections.",
  "icon": "logo/logo.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Swrnv-qc/wrppidextension.git"
  },
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Wrappid",
    "email": "wrappid.framework@gmail.com",
    "url": "https://www.github.com/wrappid"
  },
  "bugs": {
    "url": "https://github.com/Swrnv-qc/wrppidextension/issues"
  },
  "homepage": "https://github.com/Swrnv-qc/wrppidextension#readme",
  "dependencies": {
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      }
    ]
  }
